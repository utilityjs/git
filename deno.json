      "README.md",
      "deno.json",
      "deno.lock",
      "src/**/*.ts"
    ],
    "exclude": [
      "test/"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^0.220.1",
    "@std/path": "jsr:@std/path@^0.220.1",
    "@std/testing": "jsr:@std/testing@^0.220.1"
  }
}